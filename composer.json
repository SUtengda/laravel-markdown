{
    "name": "haleks/laravel-markdown",
    "description": "Laravel Markdown integrate markdown 'curly' braces inside the blade template engine, adding view extension and also giving the possibility of extending CommonMark.",
    "keywords": ["laravel", "framework", "markdown", "commonmark", "common mark", "laravel markdown", "laravel-markdown", "haleks", "Alex Villeneuve", "AlexVilleneuve", "Alexandre Villeneuve", "AlexandreVilleneuve", "haleksandre"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Villeneuve",
            "email": "haleksviller@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.0.*|5.1.*|5.2.*|5.3.*",
        "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*",
        "illuminate/view": "5.0.*|5.1.*|5.2.*|5.3.*",
        "league/commonmark": "0.10.*|0.12.*|0.13.*|0.14.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0",
        "orchestra/testbench": "~3.2",
        "webuni/commonmark-attributes-extension": "0.3.*"
    },
    "autoload": {
        "psr-4": {
            "Haleks\\Markdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Haleks\\Tests\\Markdown\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    },
    "suggest": {
        "haleks/writedown": "Abandoned in favor of the more flexible Writedown package. Which add support for multiple markdown parsers."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
